{
    "name": "negotiation",
    "version": "1.0.0",
    "description": "App based on work contract negotiation and handles expectation based on salaries by employer and employee",
    "scripts": {
      "start": "webpack-dev-server --mode=development --progress --config config/webpack.config.js",
      "build": "rimraf dist && webpack --config config/webpack.config.js --mode=production",
      "test": "mochapack --webpack-config config/webpack-test.config.js --require src/tests/setup.js src/tests/**/*.spec.js",
      "test:watch": "mochapack --watch --webpack-config config/webpack-test.config.js --require src/tests/setup.js src/tests/**/*.spec.js",
      "precommit": "lint-staged",
      "format": "./node_modules/.bin/prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss,vue,md}'"
    },
    "dependencies": {
      "vue": "^2.6.11"
    },
    "private": true,
    "devDependencies": {
      "@babel/cli": "^7.8.4",
      "@babel/core": "^7.9.0",
      "@babel/preset-env": "^7.9.5",
      "@vue/test-utils": "^1.0.0-beta.33",
      "babel-eslint": "^10.1.0",
      "babel-loader": "^8.1.0",
      "babel-plugin-syntax-dynamic-import": "^6.18.0",
      "core-js": "^3.6.5",
      "css-loader": "^3.5.2",
      "es6-promise": "^4.2.8",
      "eslint": "^6.8.0",
      "eslint-config-prettier": "^6.10.1",
      "eslint-plugin-prettier-vue": "^2.0.2",
      "eslint-plugin-vue": "^6.2.2",
      "husky": "^0.14.3",
      "lint-staged": "^7.2.0",
      "expect": "^25.3.0",
      "html-webpack-plugin": "^4.2.0",
      "jest": "^25.3.0",
      "jest-serializer-vue": "^2.0.2",
      "jsdom": "^16.2.2",
      "jsdom-global": "^3.0.2",
      "mocha": "^7.1.1",
      "mochapack": "^1.1.15",
      "node-sass": "^4.13.1",
      "prettier": "^2.0.4",
      "rimraf": "^3.0.2",
      "sass-loader": "^8.0.2",
      "sinon": "^9.0.2",
      "vue-loader": "^15.9.1",
      "vue-style-loader": "^4.1.2",
      "vue-template-compiler": "^2.6.11",
      "webpack": "^4.42.1",
      "webpack-cli": "^3.3.11",
      "webpack-dev-server": "^3.10.3",
      "webpack-node-externals": "^1.7.2"
    }
  }
  